<h1>Listing logs</h1>

<table>
  <tr>
    <th>Investigation</th>
    <th>Name</th>
    <th>Description</th>
    <th>Data type</th>
    <th>Path</th>
    <th>Time bias</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= log.investigation_id %></td>
    <td><%= log.name %></td>
    <td><%= log.description %></td>
    <td><%= log.data_type %></td>
    <td><%= log.path %></td>
    <td><%= log.time_bias %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
